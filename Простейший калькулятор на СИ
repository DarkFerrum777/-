#include <stdio.h>

int main() {
  int c, a, b;
  double x;
  scanf("%d %d %c", &a,&b,&c);
  switch(c) {
      case '+' : printf("%.2lf", x=(double)a+b); break;
      case '-' : printf("%.2lf", x=(double)a-b); break;
      case '*' : printf("%.2lf", x=(double)a*b); break;
      case '/' : 
          switch(b) {
              case 0 : printf("ERROR!"); break; // Должен выводить ERROR! если b = 0, но выводит его всегда
              default : printf("%.2lf", x=(double)a/b); break; // Считает деление, всегда, когда b не равен 0.
          } break;
      default : printf("ERROR!"); break;
  }                    
  return 0;
}
